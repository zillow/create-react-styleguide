{
  "name": "create-react-styleguide",
  "description": "A toolkit for creating React component libraries and style guides",
  "version": "5.0.0",
  "license": "ISC",
  "author": "Brian Stone <brians@zillowgroup.com>",
  "homepage": "https://github.com/zillow/create-react-styleguide#readme",
  "bugs": {
    "url": "https://github.com/zillow/create-react-styleguide/issues"
  },
  "bin": {
    "create-react-styleguide": "./lib/bin/create-react-styleguide.js"
  },
  "main": "./lib/index.js",
  "scripts": {
    "build": "src/bin/create-react-styleguide.js script build",
    "build:watch": "src/bin/create-react-styleguide.js script build:watch",
    "clean": "src/bin/create-react-styleguide.js script clean",
    "eslint": "src/bin/create-react-styleguide.js script eslint",
    "eslint:fix": "src/bin/create-react-styleguide.js script eslint:fix",
    "test": "src/bin/create-react-styleguide.js script test",
    "test:coverage": "src/bin/create-react-styleguide.js script test:coverage",
    "test:update": "src/bin/create-react-styleguide.js script test:update",
    "test:watch": "src/bin/create-react-styleguide.js script test:watch",
    "prepublishOnly": "src/bin/create-react-styleguide.js script prepublishOnly",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint && npm run test"
    }
  },
  "files": [
    "lib",
    "templates"
  ],
  "engines": {
    "node": ">=6.11.5"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "acorn": "^6.3.0",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "circular-dependency-plugin": "^5.2.0",
    "copy-template-dir": "1.4.0",
    "cross-spawn": "6.0.5",
    "eslint": "^6.5.1",
    "inquirer": "^6.5.2",
    "jest": "^24.9.0",
    "ora": "^3.4.0",
    "prettier": "^1.18.2",
    "react-styleguidist": "^9.1.16",
    "resolve": "^1.12.0",
    "resolve-pkg": "^2.0.0",
    "rimraf": "^2.7.1",
    "run-parallel": "1.1.9",
    "run-series": "1.1.8",
    "webpack": "^4.41.0",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "babel-preset-zillow": "^4.2.0",
    "eslint-plugin-zillow": "^3.4.0",
    "husky": "^3.0.8",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "standard-version": "^6.0.1",
    "tacks": "^1.3.0",
    "tempy": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zillow/create-react-styleguide.git"
  },
  "keywords": [
    "development",
    "react",
    "styleguide",
    "style guide",
    "component library",
    "tooling"
  ]
}
